{
  "name": "governance-v2-subgraph",
  "version": "1.0.0",
  "description": "This package contains governance subgraph for the Kyber Governance",
  "scripts": {
    "dev-environment": "npm i && tail -f /dev/null",
    "prepare:all": "npm run prepare:contracts && npm run prepare:subgraph",
    "prepare:subgraph": "cat ./config/${NETWORK}.json | mustache  - subgraph.template.yaml > subgraph.yaml",
    "codegen": "graph codegen --output-dir ./generated",
    "build": "graph build",
    "deploy": "graph deploy ducquangkstn/kyber_governance --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "create-local": "graph create ducquangkstn/kyber_governance --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy ducquangkstn/kyber_governance --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:aave/aave-protocol.git"
  },
  "author": "AAVE <tech@aave.com>",
  "contributors": [
    {
      "name": "Andrey Kozlov",
      "email": "andrey@aave.com"
    },
    {
      "name": "Emilio Frangella",
      "email": "emilio@aave.com"
    },
    {
      "name": "Ernesto Boado",
      "email": "ernesto@aave.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "typescript": "3.5.3"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.18.0",
    "@graphprotocol/graph-ts": "^0.20.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "lint-staged": "^9.2.5",
    "mustache": "^3.1.0",
    "prettier": "^1.18.2",
    "typescript": "3.5.3"
  }
}
